{
  "name": "TG Music Player",
  "description": "Open Source music bot code to make your own music bot. Maintained by Akhil.",
  "keywords": ["music", "voicechat", "telegram"],
  "logo": "https://te.legra.ph/file/f39b3713bda21fdb38e2d.jpg",
  "repository": "https://github.com/akhilprs/TG-MusicPlayer",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram String Session of assistant account.",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "API_ID": {
      "description": "API_ID from telegram web",
      "required": true
    },
    "API_HASH": {
      "description": "API_HASH from telegram web",
      "required": true
    },
    "SUPPORT": {
      "description": "Username of the chat where users will go and disturb you.(without @)",
      "required": true,
      "value": "BlueCodeSupport"
    },
    "OWNER_ID": {
      "description": "Your Telegram ID",
      "required": true,
      "value": "2102783671"
    },
    "BOT_USERNAME": {
      "description": "your bot Username without @",
      "required": true
    
   }
}
